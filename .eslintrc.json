{
  "env": {
    "browser": true,
    "es6": true
  },


    "settings": {
      "react": {
        "version": "999.999.999"
      }
    },

  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "indent": [ "error", 4 ],
    "linebreak-style": [ 0, "error", "windows" ],
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off"
  }
}
